{
    "name": "react-bypass",
    "version": "0.1.0",
    "description": "Element bypass/skip for React.",
    "keywords": [
        "react",
        "slot",
        "bypass"
    ],
    "homepage": "https://github.com/rortan134/react-bypass/",
    "bugs": {
        "url": "https://github.com/rortan134/react-bypass/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/rortan134/react-bypass.git"
    },
    "license": "MIT",
    "author": "GSMT <gsmt.dev@gmail.com>",
    "exports": {
        "import": {
            "types": "./dist/index.d.mts",
            "default": "./dist/index.mjs"
        },
        "require": {
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        }
    },
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "bunchee",
        "dev": "bunchee --watch",
        "dev:test": "turbo run dev --filter=test...",
        "dev:website": "turbo run dev --filter=website...",
        "format": "prettier --write .",
        "test": "vitest run",
        "test:e2e": "playwright test"
    },
    "dependencies": {
        "@radix-ui/react-slot": "^1.0.2"
    },
    "devDependencies": {
        "@playwright/test": "^1.40.1",
        "@testing-library/jest-dom": "^6.2.0",
        "@testing-library/react": "^14.1.2",
        "@types/react": "^18.2.46",
        "bunchee": "^4.2.11",
        "eslint": "^7.32.0",
        "eslint-plugin-playwright": "^0.21.0",
        "husky": "^8.0.3",
        "lint-staged": "^15.2.0",
        "prettier": "^2.8.8",
        "prettier-plugin-packagejson": "^2.4.9",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "turbo": "1.6",
        "typescript": "5.2.2",
        "vitest": "^1.2.0"
    },
    "peerDependencies": {
        "react": "^16.8 || ^17.0 || ^18.0",
        "react-dom": "^16.8 || ^17.0 || ^18.0"
    },
    "packageManager": "pnpm@6.32.11",
    "engines": {
        "node": ">=18.x",
        "npm": ">=9.0.0"
    }
}
